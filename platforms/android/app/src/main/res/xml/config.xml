<?xml version='1.0' encoding='utf-8'?>
<widget id="com.micaelil" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>CoronaUpdates</name>
    <description>
    A simple app that gives corona updates
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
      Micael Illos
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="mdpi" src="assets/mipmap-hdpi/icon.png" />
    <icon density="ldpi" src="assets/mipmap-mdpi/icon.png" />
    <icon density="hdpi" src="assets/mipmap-xhdpi/icon.png" />
    <icon density="xhdpi" src="assets/mipmap-xxhdpi/icon.png" />
    <icon density="xxhdpi" src="assets/mipmap-xxxhdpi/icon.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
